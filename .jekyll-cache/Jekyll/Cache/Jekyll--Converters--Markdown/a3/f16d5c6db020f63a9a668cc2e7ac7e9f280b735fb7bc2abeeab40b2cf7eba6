        <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì˜¤ë¦¬ ì‹œë®¬ë ˆì´ì…˜ ê²Œì„ : ì „ì²´ ë¬´ë¦¬"</span><span class="o">);</span>
        <span class="n">simulate</span><span class="o">(</span><span class="n">flockOfDucks</span><span class="o">);</span>

        <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì˜¤ë¦¬ ì‹œë®¬ë ˆì´ì…˜ ê²Œì„ : ë¬¼ì˜¤ë¦¬ ë¬´ë¦¬"</span><span class="o">);</span>
        <span class="n">simulate</span><span class="o">(</span><span class="n">flockOfMallards</span><span class="o">);</span>

