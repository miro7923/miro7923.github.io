        <span class="nc">Quackable</span> <span class="n">rubberDuck</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">QuackCounter</span><span class="o">(</span><span class="k">new</span> <span class="nc">RubberDuck</span><span class="o">());</span> <span class="c1">// Quackableì„ ë°ì½”ë ˆì´í„°ë¡œ ê°ì‹¸ ì¤Œ</span>
        <span class="nc">Quackable</span> <span class="n">goose</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">GooseAdapter</span><span class="o">(</span><span class="k">new</span> <span class="nc">Goose</span><span class="o">());</span> <span class="c1">// ì–´ëŒ‘í„°ë¥¼ ì‚¬ìš©í•˜ë©´ ì˜¤ë¦¬ë¡œ ê°ì‹¼ ê±°ìœ„ ìƒì„± ê°€ëŠ¥</span>

        <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"\nì˜¤ë¦¬ ì‹œë®¬ë ˆì´ì…˜ ê²Œì„"</span><span class="o">);</span>

        <span class="n">simulate</span><span class="o">(</span><span class="n">mallardDuck</span><span class="o">);</span>
        <span class="n">simulate</span><span class="o">(</span><span class="n">redheadDuck</span><span class="o">);</span>
        <span class="n">simulate</span><span class="o">(</span><span class="n">duckCall</span><span class="o">);</span>
