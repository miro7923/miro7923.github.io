        <span class="n">flockOfMallards</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="n">mallardFour</span><span class="o">);</span>   <span class="c1">// ë¬¼ì˜¤ë¦¬ ë¬´ë¦¬ ìƒì„±</span>

