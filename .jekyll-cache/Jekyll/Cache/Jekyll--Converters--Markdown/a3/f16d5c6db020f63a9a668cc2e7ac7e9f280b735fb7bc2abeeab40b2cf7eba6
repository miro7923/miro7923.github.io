        <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì˜¤ë¦¬ê°€ ì†Œë¦¬ ë‚¸ íšŸìˆ˜ : "</span> <span class="o">+</span> <span class="nc">QuackCounter</span><span class="o">.</span><span class="na">getDuckCnt</span><span class="o">()</span> <span class="o">+</span> <span class="s">" ë²ˆ"</span><span class="o">);</span>
